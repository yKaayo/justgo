{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "mobile/.expo/types/**/*.ts",
    "expo-env.d.ts",
    "nativewind-env.d.ts",
    "app/(tabs)/index.tsx",
    "assets/icons/LoopIcon.js",
    ".expo/types/**/*.ts"
, "constants/architectures.js", "components/Navbar.jsx"  ]
}
