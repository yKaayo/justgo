{
  "expo": {
    "name": "justgo",
    "slug": "justgo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "restate",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/icons/just-go.svg",
          "resizeMode": "cover",
          "backgroundColor": "#ffffff",
          "enableFullScreenImage": true
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Raleway-Regular.ttf",
            "./assets/fonts/Lexend-SemiBold.ttf"
          ]
        }
      ]
    ],

    "experiments": {
      "typedRoutes": true
    }
  }
}
